generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model films {
  name String?
  desc String?
  id   Int     @id @default(autoincrement())
}

model books {
  id   Int     @id @default(autoincrement())
  name String
  author String
}